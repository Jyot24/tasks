<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>All Task</title>
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/css/bootstrap.min.css"
    integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">

  <style>
    h5,
    td,
    th {
      margin-top: 20px;
      text-align: center;
      
    }

    h5{
     font-size: 70px;
    }
    
    th{
     font-size: 30px;
    }

    td{
      padding:0px 50px;
      background-color: rgb(25, 231, 231);
     font-size: 20px;
    }

    tr{
      margin: 20px 0px;
      border: 1px solid white;
      background-color: rgb(19, 159, 33);
    }
  </style>
</head>

<body>
  <%-include('component/navbar/navbar.ejs') %>
  <h5>ALL TASK</h5>
  <section class=" " id="card">
    <div class="container align-items-center h-100">
      <div id="card-items">

        <div class="row justify-content-center align-items-center ">

          <table class="center">


            <%for(let key of temp){%>
              <% const propertynames=Object.keys(key);%>
                <tr>
                  <%for(let i=0;i<propertynames.length;i++){%>

                    <th>
                     <%=propertynames[i]%>
                    </th>


                    <% } %>
                </tr>
                <% break; %>
                  <% } %>



                    <%for(let key of temp){%>
                      <% const propertyvalues=Object.values(key); %>
                      <td>
                        <%=propertyvalues[0]%>
                      </td>

                      <td>
                        <a href="<%=propertyvalues[1]%>"><%=propertyvalues[1]%></a>
                      </td>

                        <tr>
                          <!-- <%for(let i=0;i<propertyvalues.length;i++){%> -->
                          
                           
                            <!-- <% } %> -->
                        </tr>
                              <% } %>
        </div>
      </div>
    </div>

  </section>


  </table>
</body>

</html>